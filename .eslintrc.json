{
	"extends": [
		"wikimedia/server",
		"wikimedia/jsdoc"
	],
	"rules": {
		"camelcase": [
			"error",
			{
				"properties": "never"
			}
		],
		"no-underscore-dangle": "off",
		"no-unused-vars": [
			"error",
			{
				"args": "none"
			}
		],
		"jsdoc/no-undefined-types": "off"
	},
	"overrides": [
		{
			"files": [ "**/*.json", "**/*.jsonc" ],
			"extends": "wikimedia/json"
		}
	]
}
